{
  "name": "@ibm-aspera-sdk/browser",
  "version": "3.10.0-beta.1",
  "description": "Official IBM Aspera Connect SDK for Browsers",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "esm/index.js",
  "scripts": {
    "build": "run-s build:dist build:esm build:bundle build:docs",
    "build:bundle": "webpack",
    "build:bundle:watch": "webpack --watch",
    "build:dist": "tsc -p tsconfig.json",
    "build:dist:watch": "tsc -p tsconfig.json -w --preserveWatchOutput",
    "build:docs": "jsdoc build/asperaweb-4.js --configure config/jsdoc.config.js",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:esm:watch": "tsc -p tsconfig.esm.json -w --preserveWatchOutput",
    "build:watch": "run-p build:dist:watch build:esm:watch build:bundle:watch",
    "check-types": "tsc --noEmit",
    "clean": "rimraf dist build esm coverage",
    "dev": "webpack  --mode=development --progress --colors --watch",
    "lint": "tslint -p tsconfig.json -c tslint.json --format verbose",
    "test": "karma start test/karma/karma.unit.conf.js --single-run",
    "test:docker": "karma start test/karma/karma.integration.conf.js --browsers ChromeCustom",
    "test:integration": "karma start test/karma/karma.integration.conf.js",
    "test:watch": "karma start test/karma/karma.unit.conf.js --auto-watch --no-single-run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@types/aes-js": "^3.1.0",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/sinon": "^7.0.11",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "core-js": "^3.0.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jsdoc": "^3.6.3",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-junit-reporter": "^1.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-webpack": "^4.0.0-rc.6",
    "mocha": "^6.1.4",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "sinon": "^7.3.2",
    "terser-webpack-plugin": "^1.2.3",
    "ts-loader": "^5.4.4",
    "tslint": "^5.16.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.4.5",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  },
  "dependencies": {},
  "jshintConfig": {
    "esversion": 6
  }
}
